# Sourced by wasm32 scripts to set up RUSTFLAGS
CARGO_CONFIG_RUSTFLAGS=$(cargo +nightly -Z unstable-options config get 'target.cfg(target_arch="wasm32").rustflags' --format json 2>/dev/null | python3 -c "import json,sys; print(' '.join(json.load(sys.stdin)['target']['cfg(target_arch=\"wasm32\")']['rustflags']))")
export WASM32_RUSTFLAGS="$CARGO_CONFIG_RUSTFLAGS -D warnings"
